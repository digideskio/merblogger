
<script type="text/x-jquery-template" title="blogs">
<div id="blogs">
<% $.each(self, function(index, blog) { %>
  <span class="blog" blog_id="<%= blog.id %>"><%= partial("blog", blog) %></span>
<% }); %>
</div>
</script>

<script type="text/x-jquery-template" title="blog">
<div class="blog">
  <%= self.name %>
</div>
</script>
